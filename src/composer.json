    },
    {
      "type": "package",
      "package": {
        "name": "twbs/bootstrap-helper",
        "version": "1.0.1",
        "dist": {
          "url": "https://github.com/WebProWWW/bootstrap-helper/archive/1.0.1.zip",
          "type": "zip"
        }
