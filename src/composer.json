{
    "name": "gulp-web/src",
    "type": "library",
    "license": "mit",
    "minimum-stability": "stable",
    "require": {
        "jquery/jquery": "3.4.1",
        "twbs/bootstrap": "4.3.1",
        "twbs/bootstrap-helper": "1.0.1",
    },

    "repositories": [
